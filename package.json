{
  "name": "@lewis-ing/vue-keycloak-js",
  "version": "1.0.0",
  "description": "A Keycloak plugin for Vue >= 2.x",
  "main": "dist/lewis-ing-vue-keycloak.cjs.js",
  "module": "dist/lewis-ing-vue-keycloak.esm.js",
  "browser": "dist/lewis-ing-vue-keycloak.umd.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:lewis-ing/vue-keycloak-js.git"
  },
  "dependencies": {
    "keycloak-js": "8.0.0"
  },
  "keywords": [
    "vue",
    "keycloak"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "rollup": "^1.9.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.1.0"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "version": "npm run build && git add -A",
    "postversion": "git push -u --follow-tags origin master && npm publish"
  },
  "files": [
    "dist"
  ]
}
